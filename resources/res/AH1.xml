<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:chimera="http://schemas.android.com/chimera" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <solid android:color="@color/google_grey400"/>
            <corners android:radius="@dimen/as_progress_bar_rounded_corner_radius"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/google_blue600"/>
                <corners android:radius="@dimen/as_progress_bar_rounded_corner_radius"/>
            </shape>
        </clip>
    </item>
</layer-list>
