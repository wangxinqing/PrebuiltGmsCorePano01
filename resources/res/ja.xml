<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:chimera="http://schemas.android.com/chimera" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <View android:background="@drawable/gm_horizontal_line_divider" android:layout_width="match_parent" android:layout_height="@dimen/profile_card_separator_height"/>
    <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/quick_actions_bar" android:paddingTop="@dimen/header_quick_actions_bar_padding" android:paddingBottom="@dimen/header_quick_actions_bar_padding" android:layout_width="match_parent" android:layout_height="wrap_content" android:divider="@drawable/quick_action_divider" android:showDividers="none|beginning|middle|end">
        <com.google.android.material.button.MaterialButton android:enabled="false" android:id="@+id/quick_actions_email" android:layout_width="@dimen/header_quick_action_width" android:layout_height="wrap_content" android:text="@string/profile_communicate_email" android:drawableTop="@drawable/quick_action_portrait_icon_placeholder" style="?attr/borderlessButtonStyle"/>
        <com.google.android.material.button.MaterialButton android:enabled="false" android:id="@+id/quick_actions_schedule" android:layout_width="@dimen/header_quick_action_width" android:layout_height="wrap_content" android:text="@string/quick_actions_schedule" android:drawableTop="@drawable/quick_action_portrait_icon_placeholder" style="?attr/borderlessButtonStyle"/>
        <com.google.android.material.button.MaterialButton android:enabled="false" android:id="@+id/quick_actions_hangout" android:layout_width="@dimen/header_quick_action_width" android:layout_height="wrap_content" android:text="@string/quick_actions_hangout" android:drawableTop="@drawable/quick_action_portrait_icon_placeholder" style="?attr/borderlessButtonStyle"/>
        <com.google.android.material.button.MaterialButton android:enabled="false" android:id="@+id/quick_actions_call" android:layout_width="@dimen/header_quick_action_width" android:layout_height="wrap_content" android:text="@string/profile_call_type_unknown" android:drawableTop="@drawable/quick_action_portrait_icon_placeholder" style="?attr/borderlessButtonStyle"/>
    </LinearLayout>
    <View android:background="@drawable/gm_horizontal_line_divider" android:layout_width="match_parent" android:layout_height="@dimen/profile_card_separator_height"/>
</merge>
