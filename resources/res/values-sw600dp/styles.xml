<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Auth.CredentialsDialog">
        <item name="android:windowMinWidthMajor">460dp</item>
        <item name="android:windowMinWidthMinor">460dp</item>
    </style>
    <style name="ChromeSync.Dialog">
        <item name="android:windowMinWidthMajor">460dp</item>
        <item name="android:windowMinWidthMinor">460dp</item>
    </style>
    <style name="Sharing.CardActivity">
        <item name="android:textColorSecondary">@color/sharing_color_secondary_text</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:homeAsUpIndicator">@drawable/quantum_gm_ic_clear_vd_theme_24</item>
        <item name="android:fontFamily">@font/google_sans_medium</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:strokeColor">@color/sharing_stroke_color</item>
        <item name="android:colorButtonNormal">?attr/colorAccent</item>
        <item name="android:windowEnterTransition">@transition/sharing_window_enter_transition</item>
        <item name="android:windowSharedElementEnterTransition">@transition/sharing_window_shared_element_enter_transition</item>
        <item name="android:windowSharedElementExitTransition">@transition/sharing_window_shared_element_exit_transition</item>
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowSharedElementReturnTransition">@transition/sharing_window_shared_element_return_transition</item>
        <item name="android:windowSharedElementReenterTransition">@transition/sharing_window_shared_element_reenter_transition</item>
        <item name="android:windowSharedElementsUseOverlay">true</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="colorAccent">@color/sharing_color_accent</item>
        <item name="colorButtonNormal">?attr/colorAccent</item>
        <item name="colorError">@color/sharing_color_error</item>
        <item name="colorPrimary">@color/sharing_color_primary</item>
        <item name="colorPrimaryDark">@color/sharing_color_primary</item>
    </style>
    <style name="Sharing.Consents.Content">
        <item name="android:gravity">top|center_vertical</item>
        <item name="android:layout_gravity">top|left|center_vertical|center_horizontal|center</item>
        <item name="android:background">@drawable/sharing_bottom_card_bg</item>
        <item name="android:layout_width">600dp</item>
        <item name="android:minHeight">250dp</item>
    </style>
    <style name="Sharing.ReceiveSurface.Content">
        <item name="android:gravity">top|center_vertical</item>
        <item name="android:layout_gravity">top|left|center_vertical|center_horizontal|center</item>
        <item name="android:background">@drawable/sharing_bottom_card_bg</item>
        <item name="android:layout_width">600dp</item>
        <item name="android:minHeight">@dimen/sharing_receiver_surface_content_min_height</item>
    </style>
    <style name="Sharing.Setup.Content">
        <item name="android:gravity">top|center_vertical</item>
        <item name="android:layout_gravity">top|left|center_vertical|center_horizontal|center</item>
        <item name="android:background">@drawable/sharing_bottom_card_bg</item>
        <item name="android:layout_width">600dp</item>
        <item name="android:minHeight">250dp</item>
    </style>
    <style name="Sharing.ShareSheet.Content">
        <item name="android:gravity">top|center_vertical</item>
        <item name="android:layout_gravity">top|left|center_vertical|center_horizontal|center</item>
        <item name="android:background">@drawable/sharing_bottom_card_bg</item>
        <item name="android:layout_width">600dp</item>
        <item name="android:minHeight">300dp</item>
    </style>
    <style name="Widget.Design.TabLayout">
        <item name="tabGravity">1</item>
        <item name="tabMode">1</item>
    </style>
    <style name="common.Widget.Base.Snackbar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">568dp</item>
        <item name="android:minWidth">288dp</item>
        <item name="windowGravity">53</item>
        <item name="windowWidth">-2</item>
        <item name="windowXOffset">16dp</item>
        <item name="windowYOffset">16dp</item>
    </style>
</resources>
